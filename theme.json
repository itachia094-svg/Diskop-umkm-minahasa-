	"styles": {
		"blocks": {
			"core/button": {
				"variations": {
					"outline": {
						"border": {
							"color": "var(--wp--preset--color--primary)",
							"style": "solid",
							"width": "1px"
						},
						"color": {
							"text": "var(--wp--preset--color--primary)"
						},
						"css": ".wp-block-button__link:not(.has-background):hover {background:var(--wp--preset--color--primary);color: var(--wp--preset--color--default) !important;}",
						"spacing": {
							"padding": {
								"bottom": "15px",
								"left": "var(--wp--preset--spacing--30)",
								"right": "var(--wp--preset--spacing--30)",
								"top": "15px"
							}
						}
					},
					"scroll-to-top": {
						"css": ".wp-element-button{font-size:inherit;position:fixed;right:15px;bottom:15px;z-index:999;text-indent:-5000px;width:50px !important;height:50px;opacity:0;visibility:hidden;transition:opacity 0.3s,visibility 0.3s;}"
					}
				}
			},
			"core/categories": {
				"color": {
					"text": "var(--wp--preset--color--tertiary)"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--base)"
						}
					}
				}
			},
			"core/code": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-small)",
					"lineHeight": "1.3"
				}
			},
			"core/comment-author-name": {
				"color": {
					"text": "var(--wp--preset--color--base)"
				},
				"elements": {
					"link": {
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--primary)"
							}
						},
						"color": {
							"text": "var(--wp--preset--color--base)"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-small)",
					"fontWeight": "600"
				}
			},
			"core/comment-content": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-small)"
				}
			},
			"core/comment-date": {
				"elements": {
					"link": {
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--primary)"
							}
						},
						"color": {
							"text": "var(--wp--preset--color--tertiary)"
						}
					}
				},
				"typography": {
					"fontSize": "13px"
				}
			},
			"core/comment-edit-link": {
				"elements": {
					"link": {
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--quaternary)"
							}
						},
						"color": {
							"text": "var(--wp--preset--color--primary)"
						}
					}
				},
				"typography": {
					"fontSize": "13px"
				}
			},
			"core/comment-reply-link": {
				"elements": {
					"link": {
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--quaternary)"
							}
						},
						"color": {
							"text": "var(--wp--preset--color--primary)"
						}
					}
				},
				"spacing": {
					"margin": {
						"top": "15px"
					}
				},
				"typography": {
					"fontSize": "13px"
				}
			},
			"core/comment-template": {
				"spacing": {
					"margin": {
						"bottom": "var(--wp--preset--spacing--30)"
					},
					"padding": {
						"bottom": "var(--wp--preset--spacing--40)",
						"top": "var(--wp--preset--spacing--40)"
					}
				}
			},
			"core/comments-pagination": {
				"css": "& .page-numbers:is(.dots, .current){padding:var(--wp--preset--spacing--10) 15px; border:1px solid #f5f5f5; border-radius:5px;} & .current{background:var(--wp--preset--color--primary); color: var(--wp--preset--color--default);} ",
				"elements": {
					"link": {
						":hover": {
							"border": {
								"color": "var(--wp--preset--color--primary)",
								"radius": "5px"
							},
							"color": {
								"background": "var(--wp--preset--color--primary)",
								"text": "var(--wp--preset--color--default)"
							},
							"typography": {
								"textDecoration": "none"
							}
						},
						"border": {
							"color": "#f5f5f5",
							"radius": "5px",
							"style": "solid",
							"width": "1px"
						},
						"color": {
							"text": "var(--wp--preset--color--base)"
						},
						"spacing": {
							"margin": {
								"bottom": "2px",
								"left": "2px",
								"right": "2px",
								"top": "2px"
							},
							"padding": {
								"bottom": "var(--wp--preset--spacing--10)",
								"left": "15px",
								"right": "15px",
								"top": "var(--wp--preset--spacing--10)"
							}
						}
					}
				}
			},
			"core/details": {
				"variations": {
					"custom-details": {
						"css": "& summary { padding: 15px 15px 15px 20px; background: var(--wp--preset--color--tertiary); font-weight: bold; border-radius: 10px;} & summary:hover{background: var(--wp--preset--color--secondary); color:var(--wp--preset--color--default)} & summary::marker { content: '\\002B  ';  } &[open] summary::marker{content: '\\2212  ';} &[open] summary{background: var(--wp--preset--color--secondary); color:var(--wp--preset--color--default)}"
					}
				}
			},
			"core/html": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--base)",
					"fontSize": "var(--wp--preset--font-size--small)",
					"lineHeight": "1.3"
				}
			},
			"core/latest-comments": {
				"color": {
					"text": "var(--wp--preset--color--tertiary)"
				},
				"css": "& {padding-left:0;}",
				"elements": {
					"link": {
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--primary)"
							}
